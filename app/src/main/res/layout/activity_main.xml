<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.travelexperts.temobile.MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="129dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="42dp"
        android:src="@drawable/logo"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:layout_width="453dp"
        android:layout_height="154dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:id="@+id/relativeLayout">

        <ImageButton
            android:id="@+id/ibAgent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="30dp"
            android:src="@drawable/agent_xhdpi"
            tools:layout_editor_absoluteX="39dp"
            tools:layout_editor_absoluteY="239dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:text="Agents"
            android:textColor="@color/colorPrimary"
            android:textSize="50sp"
            tools:layout_editor_absoluteX="324dp"
            tools:layout_editor_absoluteY="247dp"
            android:layout_marginStart="32dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/ibAgent" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="453dp"
        android:layout_height="154dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:id="@+id/relativeLayout2">

        <ImageButton
            android:id="@+id/ibCustomer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/customer_xhdpi"
            tools:layout_editor_absoluteX="125dp"
            tools:layout_editor_absoluteY="382dp"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="32dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:text="Customers"
            android:textColor="@color/colorPrimary"
            android:textSize="50sp"
            tools:layout_editor_absoluteX="295dp"
            tools:layout_editor_absoluteY="407dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/ibCustomer"
            android:layout_marginStart="30dp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="453dp"
        android:layout_height="154dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout2"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp">

        <ImageButton
            android:id="@+id/ibSupplier"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/supplier_xhdpi"
            tools:layout_editor_absoluteX="125dp"
            tools:layout_editor_absoluteY="552dp"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="36dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:text="Suppliers"
            android:textColor="@color/colorPrimary"
            android:textSize="50sp"
            tools:layout_editor_absoluteX="295dp"
            tools:layout_editor_absoluteY="565dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/ibSupplier"
            android:layout_marginStart="25dp" />

    </RelativeLayout>

    <TextView
        android:id="@+id/tvUpdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update My Information"
        android:textColor="@android:color/holo_orange_dark"
        android:textSize="30sp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

</android.support.constraint.ConstraintLayout>
